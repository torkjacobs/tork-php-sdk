{
    "name": "tork/governance",
    "description": "On-device AI governance SDK - PII detection, redaction, and cryptographic receipts",
    "type": "library",
    "license": "MIT",
    "keywords": ["ai", "governance", "pii", "redaction", "compliance", "security"],
    "authors": [
        {
            "name": "Tork Network",
            "email": "hello@tork.network"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Tork\\Governance\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tork\\Governance\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Tork\\Governance\\Laravel\\TorkServiceProvider"
            ]
        }
    }
}
